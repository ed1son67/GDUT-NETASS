/**index.wxss**/
.container {
  margin: 0;
  padding: 0;
  border: none;
  height: 100vh;
  width: 100%;
  background-color: #f7f7f7;
}
.header-container {
  color: #fff;
  margin: 0 auto;
  width: 100%;
  height: 480rpx;
  background: url(data:image/png;base64,)no-repeat;
  background-size: cover;
  position: relative;
  background-position: 0 -20px;
}
.header-container image {
  height: 400rpx;
  width: 400rpx;
  position: absolute;
  right: -20rpx;
  top: -10rpx;
}
.title-container {
  font-size: 56rpx;
  position: absolute;
  top: 50rpx;
  left: 30rpx;
  line-height: 1.5em;
  font-weight: 500;
  letter-spacing: 4rpx;

}
.small-title {
  font-size: 24rpx;
  letter-spacing: 0rpx;
  padding-top: 20rpx;
  line-height: 1em;


}
.item {
  height: 100rpx;
  margin: 40rpx;
  border-radius: 6rpx;
  background-color: #fff;
  box-shadow: #c7c7c7 0px 0px 10rpx;
}