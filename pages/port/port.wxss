.header-container {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  position: relative;
  background-position: 0 -40px;
  color: #fff;
  margin: 0 auto;
  width: 100%;
  height: 600rpx;
}

.main-container {
  position: absolute;
  height: 500rpx;
  top: 280rpx;
  border-radius: 20rpx;
  display: flex;
  background-color: #fff;
  left: 20rpx;
  right: 20rpx;
  box-shadow: #c7c7c7 0px 0px 10rpx;
  flex-direction: column;
  justify-content: center;
}
.form-contaienr {
 height: auto;
 margin: 0 auto;
}
.form-item {
  display: flex;
  padding: 12rpx 0;
  margin: 0 auto;
  align-items: center;
}
.label {
  width: 180rpx;
  text-align: left;
}
.item {
  
  text-align: left;
  width: 350rpx;
}
.button-container {
  position:absolute;
  top: 450rpx;
  left: 0;
  right: 0;
}
.button-container button {
  width: 450rpx;
  border-radius: 40rpx;
  font-size: 36rpx;
  color: #fff;
  border: none;
  letter-spacing: 4rpx;
  background-image: linear-gradient(to right, #b224ef 0%, #227bff 100%);
  
}
.title-container {
  text-align: center;
  padding: 40rpx;
  font-size: 46rpx;
  letter-spacing: 4rpx;
}


